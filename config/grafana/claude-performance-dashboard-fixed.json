          {
            "expr": "sum(rate({service=\"claude-telemetry\"}[$__range])) * 60",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            },
            "legendFormat": "Total Ops/Min"
