        "title": "🔧 Tool Distribution",
        "type": "stat",
        "description": "Most active tool",
        "targets": [
          {
            "expr": "topk(1, sum by (tool) (count_over_time({service=\"claude-telemetry\"}[$__range])))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "fixed",
