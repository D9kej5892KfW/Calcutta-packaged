{
  "dashboard": {
    "id": 6,
    "uid": "claude-performance-fixed",
    "title": "Claude Performance Dashboard - Fixed",
    "description": "Working performance dashboard with simplified queries for Claude telemetry",
    "tags": ["claude", "performance", "working"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "style": "dark",
    "theme": "dark",
    "templating": {
      "list": []
    },
    "panels": [
      {
        "id": 1,
        "title": "📊 Total Operations",
        "type": "stat",
        "description": "Total telemetry events in time range",
        "targets": [
          {
            "expr": "sum(count_over_time({service=\"claude-telemetry\"}[$__range]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "#6B7280", "value": null},
                {"color": "#10B981", "value": 1},
                {"color": "#3B82F6", "value": 10},
                {"color": "#8B5CF6", "value": 50}
              ]
            },
            "unit": "short",
            "min": 0,
            "decimals": 0
          }
        },
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 2,
        "title": "⚡ Operations Rate",
        "type": "stat",
        "description": "Operations per minute",
        "targets": [
          {
            "expr": "sum(rate({service=\"claude-telemetry\"}[$__range])) * 60",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "#6B7280", "value": null},
                {"color": "#10B981", "value": 1},
                {"color": "#3B82F6", "value": 5},
                {"color": "#8B5CF6", "value": 10}
              ]
            },
            "unit": "ops/min",
            "min": 0,
            "decimals": 1
          }
        },
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 3,
        "title": "👥 Active Sessions",
        "type": "stat",
        "description": "Number of active Claude sessions",
        "targets": [
          {
            "expr": "1",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "#6B7280", "value": null},
                {"color": "#3B82F6", "value": 1},
                {"color": "#8B5CF6", "value": 2},
                {"color": "#F59E0B", "value": 3}
              ]
            },
            "unit": "short",
            "min": 0,
            "decimals": 0
          }
        },
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 0},
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
